      function populateDistricts() {
        // Add your JavaScript logic to fetch districts based on the selected state
        // You can use an API or pre-defined data to populate the districts dynamically
        // For now, let's assume a static list for each state
        const stateSelect = document.getElementById("state");
        const districtSelect = document.getElementById("district");

        // Clear previous options
        districtSelect.innerHTML =
          '<option disabled value="" selected>Select a district</option>';

        // Get selected state
        const selectedState = stateSelect.value;

        // Define districts based on the selected state
        const districtOptions = getDistricts(selectedState);

        // Add district options to the select element
        districtOptions.forEach((district) => {
          const option = document.createElement("option");
          option.value = district;
          option.text = district;
          districtSelect.appendChild(option);
        });
      }

